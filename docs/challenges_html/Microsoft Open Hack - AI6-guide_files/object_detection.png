    <div class="modal fade bs-example-modal-md" tabindex="-1" role="dialog" id="setCoursePlanPop" aria-labelledby="mySmallModalLabel" aria-hidden="true" style="z-index:9999;">
        <div class="modal-dialog modal-sm" style="z-index:9999;">
            <div class="modal-content">
                <div class="">
                    <div class="modal-header cancelSubscriptionHead" style="border-bottom: 1px solid #000000;">
                        <button type="button" class="close popup_btn" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                        <h2 class="modal-title" id="setCoursePlanPop-header">ADD TO COURSE PLAN</h2>
                    </div>
                    <div class="container no_padding" style="width:100% !important;">
                        <div class="content_bx">
                            <div class="modal-body">
                                <div class="col-sm-6 no_padding">
                                    <h4 id="confirmationMessage">Goal Completion Date</h4>
                                    
                                    <input type="date" id="goalDate" class="ota-datepicker" min="2019-01-13" value="2019-01-13" style="margin-bottom: 5px;"/>
                                    <span class="field-validation-error hide" id="goalDateValidation">The Goal Completition Date is not valid</span>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="modal-footer">
                                <div class="button popupButtonAlign">
                                    <button type="button" class="btn__purple btnSaveGoalDate popup_btn" id="btnSaveGoalDate">Save</button>
                                    <button type="button" class="btn__purple btnCancelGoalDate popup_btn" data-dismiss="modal">Cancel</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade " tabindex="-1" role="dialog" id="accountConformation" aria-labelledby="mySmallModalLabel" aria-hidden="true" style="z-index:9999;">
        <div class="modal-dialog" style="z-index:9999;">
            <button type="button" class="close lg_closebtn" data-dismiss="modal" style="font-size:33px;z-index: 9999;"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <div class="modal-content">
                <div class="">
                    <div class="modal-header heading-background" style="border-bottom: 1px solid #000000;">
                        <h3 class="modal-title" id="confirmation-header">Account Confirmation</h3>
                    </div>
                    <div class="modal-body">
                        <h4 id="confirmationMessage">Your account has been successfully created!</h4>



                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn__purple largeFont" data-dismiss="modal">OK<span class="sr-only">Close</span></button>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });

        function LoginStarted() {
            $("[ops-login-submitbutton]").addClass("hide");
            $("[ops-login-messagearea]").addClass("hide");
            $("[ops-login-spinner]").removeClass("hide");
        }

        function LoginCallback(data) {
            if (data.IsSuccess == true) {
                $(".ops-login-box").removeClass("active");
                setTimeout(function () {
                    location.reload();
                }, 500)
            }
            else {
                $("[ops-login-submitbutton]").removeClass("hide");
                $("[ops-login-messagearea]").removeClass("hide");
                $("[ops-login-spinner]").addClass("hide");
            }
        }

        function ForgotPasswordStarted() {
            $("[ops-forgot-password-submitbutton]").addClass("hide");
            $("[ops-forgot-password-messagearea]").addClass("hide");
            $("[ops-forgot-password-spinner]").removeClass("hide");
        }

        function ForgotPasswordCallback(data) {
            $("[ops-forgot-password-submitbutton]").addClass("hide");
            $("[ops-forgot-password-messagearea]").removeClass("hide");
            $("p", $("[ops-forgot-password-messagearea]")).html(data.Message);
            $("[ops-forgot-password-spinner]").addClass("hide");
        }

    </script>


    
</body>
</html>
