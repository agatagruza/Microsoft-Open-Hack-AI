        $(document).ready(function () {
            $("body").on("click", "[login-trigger]", function () {
                var fpbox = $(".ops-forgot-password-box");
                if ($(fpbox).hasClass("active")) {
                   $(fpbox).removeClass("active");
                    setTimeout(function() {
                        $("[ops-login-submitbutton]").removeClass("hide");
                        $("[ops-login-messagearea]").addClass("hide");
                        $("[ops-login-spinner]").addClass("hide");
                        $(".ops-login-box").addClass("active");
                    }, 500);
                }
                else {
                    $("[ops-login-submitbutton]").removeClass("hide");
                    $("[ops-login-messagearea]").addClass("hide");
                    $("[ops-login-spinner]").addClass("hide");
                    $(".ops-login-box").toggleClass("active");
                }
            });

            $("body").on("click", "[forgot-password-trigger]", function() {
                $(".ops-login-box").removeClass("active");
                $("[ops-forgot-password-submitbutton]").removeClass("hide");
                $("[ops-forgot-password-messagearea]").addClass("hide");
                $("[ops-forgot-password-spinner]").addClass("hide");

                setTimeout(function() {
                    $(".ops-forgot-password-box").addClass("active");
                }, 500);
            });

            $("body").on("click", "[login-box-close]", function() {
                $(".ops-login-box").removeClass("active");
            });

            $("body").on("click", "[forgot-password-box-close]", function() {
                $(".ops-forgot-password-box").removeClass("active");
            });

            $("body").on("click", "[login-sidebar-trigger]", function() {
                $("#exit").trigger("click");
                $(".ops-login-box").addClass("active");
            });

            $("body").on("click", "[search-trigger]", function () {
                $(".ops-search-box").toggleClass("active");
            });
            $("#ops-search-box").on("change", function() {
                submitSearch();
            });
            $(".ops-search-box-addon").on("click", function() {
                submitSearch();
            });
            function submitSearch() {
                var value = $("#ops-search-box").val();
                if (value && value.length > 0) {
                    window.location = "/courses/all?search=" + value + "&selection=AOD";
                }
            }

            if ($('#AuthenticatedUserName') && $('#AuthenticatedUserName').val()) {
                appInsights.setAuthenticatedUserContext($('#AuthenticatedUserName').val());
            }
            else {
                appInsights.clearAuthenticatedUserContext();
            }

            appInsights.trackPageView();

            opsgility.blockUI("#bodyContent", true);
            opsgility.ShowLoginPopup();
            subscriptionListViewModel.loadSubscriptionId();

            $('#loginFormDrop').keypress(function (e) {
                if (e.which == 13) {
                    $(this).blur();
                    $('#signIn').focus().click();
                    return false;
                }
            });

            $('#loginFormPage').keypress(function (e) {
                if (e.which == 13) {
                    $(this).blur();
                    $('#signInPage').focus().click();
                    return false;
                }
            });
            $('#loginFormPopUp').keypress(function (e) {
                if (e.which == 13) {
                    $(this).blur();
                    $('#signInPopUp').focus().click();
                    return false;
                }
            });

            $('#registrationForm').keypress(function (e) {
                if (e.which == 13) {
                    $(this).blur();
                    $('.signmeUpRegistration').focus().click();
                    return false;
                }
            });
            function keypressHandler(e) {
                if (e.which == 13) {
                    $('#forgetpsw_btn').focus().click();//give your submit an ID
                }
            }
            $('#forgetPasswordForm').keypress(keypressHandler);

            var hasValidSubscription = subscriptionListViewModel.hasValidSubscription();
            if (!hasValidSubscription) {
                $("#subscriptionTab").removeClass('hidden');
                $("#subscriptionTab").show();
                $("#freeTrialDiv").removeClass('hidden');
                $("#freeTrialDiv").show();
            }

            //Pre load images.
            var baseurl = opsgility.getBaseURL();
            var my_image = new Image();
            my_image.src = baseurl + '/content/tenant/smu/Images/login-bg.jpg';


            $('#search_form').keydown(function () {
                if (event.keyCode == 13) {
                    opsgility.showLoadingIcon('.search--bg');
                    // As ASCII code for ENTER key is "13"
                    $('#search_form').submit(); // Submit form code
                }
            });


        });
        var config = {
            contextPath: '/'
        };
    </script>

